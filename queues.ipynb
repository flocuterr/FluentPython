   "source": [
    "from collections import deque\n",
    "\n",
    "dq = deque(range(10),maxlen=10)\n",
    "print(dq)\n",
    "dq.rotate(3)\n",
    "print(dq)\n",
    "dq.rotate(-4)\n",
    "print(dq)\n",
    "dq.append(-1)\n",
    "print(dq)\n",
    "dq.appendleft(-1)\n",
    "print(dq)\n",
    "dq.extend([11,22,33])\n",
    "print(dq)\n",
    "dq.extendleft([10,20,30,40])\n",
    "print(dq)\n",
    "dq.__delitem__(1)\n",
    "print(dq)"
   ]
